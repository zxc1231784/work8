package java8_1;


public class java8_1 {


	static void sunroutine(String[] ar) {

		int sum = 0;

		for (int i = 0; i < ar.length; i++)
			try {
				{
					sum += Integer.parseInt(ar[i]);
				}
			} catch (NumberFormatException e) {

			}

		System.out.println(sum);
	}

	public static void main(String args[]) {


		sunroutine(args);

	}

}
